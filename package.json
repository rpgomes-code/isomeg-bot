{
  "name": "isomeg-bot",
  "version": "1.0.0",
  "description": "Isomeg is a Discord bot made with DiscordJs & Sapphire ",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc && node dist/index.js",
    "dev:watch": "concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "dev:build": "tsc --watch",
    "start": "node dist/index.js",
    "build": "tsc",
    "clean": "rmdir /s /q dist"
  },
  "keywords": [],
  "author": "Rui Pedro Gomes (RPGOMES)",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@sapphire/framework": "^5.3.6",
    "@sapphire/pieces": "^4.4.1",
    "discord.js": "14.x",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  }
}
